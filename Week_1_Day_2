        <h2>Main, Headers and Footers</h2>
        <article>
            <header>
                <h1>Header Artikel</h1>
                <p>Selamat datang</p>
            </header>
            <main>
                <p>ini konten utama</p>
            </main>
            <footer>
                <p>Bye!</p>
            </footer>
        </article>
        <hr>
        <article>
            <section class="header">
                <h1>Header Artikel</h1>
                <p>Selamat datang!</p>
            </section>
            <section class="main">
                <p>Ini konten utama.</p>
            </section>
            <section class="footer">
                <p>Bye</p>
            </section>
        </article>
